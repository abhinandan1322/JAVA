public  class AddTwoNumbersGUI {
    public static class Node{
        int data;
        Node next;
        Node(int data){
            this.data=data;
            this.next=null;
        }
    }

    public static class Stack{
        public static Node head;
        public static boolean isEmpty(){
            return head==null;
        }
        
        public static void push(int data){
            Node newNode = new Node(data);
            if(isEmpty()){
                head=newNode;
                return;
            }
            newNode.next=head;
            head=newNode;
        }

        public static int pop(){
            if(isEmpty()){
                return -1;
            }
            int top = head.data;
            head=head.next;
            return top;
        }

        public static int peek(){
            if(isEmpty()){
                return -1;
            }
            return head.data;
        }

        public static void main(String[] args) {
            Stack.push(1);
            Stack.push(2);
            Stack.push(3);
            Stack.push(4);
            Stack.push(5);

            while (!Stack.isEmpty()) {
                System.out.println(Stack.peek());
                Stack.pop();
            }

        }
    }
    
}
