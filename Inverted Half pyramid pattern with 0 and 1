import java.util.Scanner;

public class pattern9 {

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int rows = sc.nextInt();
        for(int i=0;i<=rows;i++){
            for(int j=1;j<=i;j++){
                if((i+j) % 2 ==0){
                    System.out.print("1");
                }
                else{
                    System.out.print("0");
                }
            }
            System.out.println();
        }
    }
}
