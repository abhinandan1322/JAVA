class Solution {
    ArrayList<Integer> valueEqualToIndex(int arr[], int n) {
        // code here
        ArrayList<Integer> res = new ArrayList<>();
        if(n==1){
            
            res.add(1);
        }
        for(int i=0;i<n;i++){
            if(arr[i]==i+1){
                res.add(arr[i]);
            }
        }
        return res;
    }
}
