class Solution {

    // Just print the space separated array elements
    void printArray(int arr[], int n) {
        // code here

        // Scanner sc = new Scanner(System.in);
        // n= sc.nextInt();
        // for(int i=0;i<n;i++){
        //     arr[i] = sc.nextInt();
        // }

        for(int i=0; i<n; i++) {
            System.out.print(arr[i] + " ");
        }
    }
}
