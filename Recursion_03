import java.util.Scanner;

public class reccursion1 {
    public static void printSum(int i, int n, int sum){

        if(i==n){
            sum+=n;
            System.out.println(sum);
            return;
        }
        sum+=i;
        printSum(i+1, n, sum);
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int i=1;

        int n=sc.nextInt();
        int sum=0;
        printSum(1, 5, 0);
        
    }
}
