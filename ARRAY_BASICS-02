class Solve {
    int[] getMoreAndLess(int[] arr, int n, int x) {
        int smaller=0;
        int greater=0;
        
        for(int i=0;i<n;i++){
            if(arr[i]<=x){
                smaller++;
            }
            if(arr[i]>=x){
                greater++;
            }
            
        }
        int[] res = new int[2];
        res[0]=smaller;
        res[1]=greater;
        return res;
    }
}
